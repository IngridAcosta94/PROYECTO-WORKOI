@page
@model RhUI.Pages.Proyect.IndexLoginModel
@{
	ViewData["Title"] = "Home page";
}

	<header>
		<meta charset="utf-8">
	</header>


<div>
	@Model.Message
</div>
<form method="post">
	<div class="form-group">
		<label asp-for="UserName" class="col-form-label col-md-2"></label>
		<div class="col-md-10">
			<input type="email" asp-for="UserName" />
		</div>
	</div>
	<div class="form-group">
		<label asp-for="Password" class="col-form-label col-md-2"></label>
		<div class="col-md-10">
			<input type="password" asp-for="Password" />
		</div>
	</div>
	<div class="form-group">
		<button class="btn btn-outline-primary btn-sm">Log in</button>
	</div>
</form>